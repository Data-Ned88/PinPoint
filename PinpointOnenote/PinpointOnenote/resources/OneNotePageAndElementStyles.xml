<?xml version="1.0" encoding="utf-8" ?>
<StylingBank>
  <BaseStyles name="Base"><!-- Things that never change -->
    <SectionTabCol>#F6B078</SectionTabCol>
    <Font>Calibri</Font>
    <SpaceBefore>0.0</SpaceBefore>
    <SpaceAfter>0.0</SpaceAfter>
    <TwoFactorTrue>#2AD00E</TwoFactorTrue>
    <TwoFactorFalse>#FF8181</TwoFactorFalse>
  </BaseStyles>
  <TableSizings>
    <!-- Font weights for table titles are given as CSS style properties in the CDATA value -->
    <TableSizing name="Small" default="0" fontSizeSectionHead="12.0" fontSizeTableHead="10.0" fontSizeText="9.0" fontSizeLineBreak="10.0" />
    <TableSizing name="Small_Regular" default="0" fontSizeSectionHead="13.0" fontSizeTableHead="12.0" fontSizeText="10.0" fontSizeLineBreak="10.0" />
    <TableSizing name="Regular" default="1" fontSizeSectionHead="14.0" fontSizeTableHead="13.0" fontSizeText="11.0" fontSizeLineBreak="10.5" />
    <TableSizing name="Large_Regular" default="0" fontSizeSectionHead="16.0" fontSizeTableHead="14.0" fontSizeText="12.0" fontSizeLineBreak="10.5" />
    <TableSizing name="Large" default="0" fontSizeSectionHead="18.0" fontSizeTableHead="16.0" fontSizeText="14.0" fontSizeLineBreak="10.5" />
  </TableSizings>
  <ColorThemes>
    <!-- These are all based on Excel table styles -->
    <ColorTheme name="Grey" default="1" titleShade="#000000" mainShade="#D9D9D9" alternateShade="#FFFFFF"  titleColor="#FFFFFF"/>
    <ColorTheme name="Blue" default="0" titleShade="#4472C4" mainShade="#D9E1F2" alternateShade="#FFFFFF" titleColor="#FFFFFF"/>
    <ColorTheme name="Green" default="0" titleShade="#70AD47" mainShade="#E2EFDA" alternateShade="#FFFFFF" titleColor="#FFFFFF"/>
    <ColorTheme name="Yellow" default="0" titleShade="#FFC000" mainShade="#FFE699" alternateShade="#FFF2CC" titleColor="#000000"/>
  </ColorThemes>
  <ColumnsWidthSettings>
    <Column tableTemplate="PasswordBank" index="0" customWidth="150.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="1" customWidth="75.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="2" customWidth="175.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="3" customWidth="175.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="4" customWidth="175.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="5" customWidth="40.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="6" customWidth="100.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="7" customWidth="125.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="8" customWidth="125.00"  customWidthType="cap_at"/>
    <Column tableTemplate="PasswordBank" index="9" customWidth="75.00"  customWidthType="cap_at"/>
  </ColumnsWidthSettings>
  <StrengthScoreFormats>
    <StrengthScoreType typeName="NotSet">
      <ScoreFormat score="-99" cellShade="#96EDFE"/>
    </StrengthScoreType>
    <StrengthScoreType typeName="PinFour">
      <ScoreFormat score="0" cellShade="#F8696B" scoreText=""/>
      <ScoreFormat score="16" cellShade="#FB9476" scoreText=""/>
      <ScoreFormat score="32" cellShade="#FCCF82" scoreText=""/>
      <ScoreFormat score="48" cellShade="#FFEB84" scoreText=""/>
      <ScoreFormat score="100" cellShade="#86FB57" scoreText=""/>
    </StrengthScoreType>
    <StrengthScoreType typeName="PinSix">
      <ScoreFormat score="0" cellShade="#F8696B" scoreText=""/>
      <ScoreFormat score="16" cellShade="#FB9476" scoreText=""/>
      <ScoreFormat score="32" cellShade="#FCCF82" scoreText=""/>
      <ScoreFormat score="48" cellShade="#FFEB84" scoreText=""/>
      <ScoreFormat score="100" cellShade="#86FB57" scoreText=""/>
    </StrengthScoreType>
    <StrengthScoreType typeName="Password">
      <ScoreFormat score="0" cellShade="#F8696B" scoreText="Crackable in 5 seconds"/>
      <ScoreFormat score="8" cellShade="#FA7F71" scoreText="Crackable in 30 mins"/>
      <ScoreFormat score="16" cellShade="#FB9476" scoreText="Crackable within 3 hours"/>
      <ScoreFormat score="24" cellShade="#FCAA7C" scoreText="Crackable in 1 day"/>
      <ScoreFormat score="32" cellShade="#FCCF82" scoreText="Crackable in 3 days"/>
      <ScoreFormat score="40" cellShade="#FEE587" scoreText="Crackable in 1 week"/>
      <ScoreFormat score="48" cellShade="#FFEB84" scoreText="Crackable in 1 month"/>
      <ScoreFormat score="56" cellShade="#FBEB89" scoreText="Crackable in 1 year"/>
      <ScoreFormat score="64" cellShade="#FAF28A" scoreText="Crackable in 5 years"/>
      <ScoreFormat score="72" cellShade="#F3F88C" scoreText="Crackable in 10 years"/>
      <ScoreFormat score="80" cellShade="#E2F97F" scoreText="Crackable in 30 years"/>
      <ScoreFormat score="88" cellShade="#A9FA72" scoreText="Crackable in 200 years"/>
      <ScoreFormat score="96" cellShade="#86FB57" scoreText="Over 200 years to crack"/>
    </StrengthScoreType>
    <StrengthScoreType typeName="PasswordBankAggregate">
      <ScoreFormat maxScore="unlimited" cellShade="#F8696B" scoreText="Major vulnerability"/>
      <ScoreFormat maxScore="7000" cellShade="#FA7F71" scoreText="Serious vulnerability"/>
      <ScoreFormat maxScore="5000" cellShade="#FB9476" scoreText="Significant vulnerability"/>
      <ScoreFormat maxScore="4000" cellShade="#FCAA7C" scoreText="Some serious vulnerability"/>
      <ScoreFormat maxScore="3500" cellShade="#FCCF82" scoreText="Medium-significant vulnerability"/>
      <ScoreFormat maxScore="2500" cellShade="#FEE587" scoreText="Some significant vulnerability"/>
      <ScoreFormat maxScore="2000" cellShade="#FFEB84" scoreText="Some medium-significant vulnerability"/>
      <ScoreFormat maxScore="1500" cellShade="#FBEB89" scoreText="Some vulnerability"/>
      <ScoreFormat maxScore="1000" cellShade="#FAF28A" scoreText="Moderate vulnerability"/>
      <ScoreFormat maxScore="700" cellShade="#F3F88C" scoreText="Some moderate vulnerability"/>
      <ScoreFormat maxScore="500" cellShade="#E2F97F" scoreText="Low-moderate vulnerability"/>
      <ScoreFormat maxScore="300" cellShade="#A9FA72" scoreText="Low vulnerability"/>
      <ScoreFormat maxScore="100" cellShade="#86FB57" scoreText="Very low vulnerability"/>
    </StrengthScoreType>    
  </StrengthScoreFormats>
  <SizingConverters>
    <SizingConverter fontSize="9.0">
      <SizingConverterFont fontName="Arial" charWidth="3.83" charWidthBold="4.12"/>
      <SizingConverterFont fontName="Calibri" charWidth="3.54" charWidthBold="3.62"/>
